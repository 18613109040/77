.contaner{
  width: 100%;
  overflow-y: scroll;
  background: #f4f4f4;
  overflow-x: hidden;
}
.section{
  padding:0 0rpx 0 30rpx;
  background: #fff;
  font-size: 16px;
}
.rate-star{
  margin: 0;
  padding: 0;
  display: inline-block;
  margin-right: 8px;
  position: relative;
  -webkit-transition: all .3s;
  transition: all .3s;
  color: #e9e9e9;
 
}
.map-info{
   position: relative;
}

.ant-rate-star-first{
      position: absolute;
    left: 0;
    top: 0;
    width: 50%;
    height: 100%;
    overflow: hidden;
    opacity: 0;
}
.ant-rate-star-second{

    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    -webkit-transition: all .3s;
    transition: all .3s;
}

.section .ic{
  width: 34rpx;
  height: 34rpx;
  margin-right: 10rpx;
}
.section  .item+.item{
  border-top: 1px solid #f4f4f4;
}
.section  .item{
  height: 100rpx;
  line-height: 100rpx;
}
.section  .item text{
  display: inline-block;
}
.section + .section{
  margin-top: 20rpx;
}
