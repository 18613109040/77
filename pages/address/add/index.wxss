.sb-drawer_screen{
   width: 100%;
 height: 100%;
 position: fixed;
 top: 0;
 left: 0;
 z-index: 1000;
 background: rgba(0, 0, 0, 0.5);
 overflow: hidden;
}
.sb-drawer_pick{
 overflow: hidden;
 position: fixed;
 z-index: 1001;
 background: #fff;
 padding: 20rpx;
 width: 100%;
 bottom: 0;
}
.pick-header{
  display: flex;
  width: 100%;
}
.pick-header .cancel{
  flex: 1;
  text-align: left;
}
.pick-header .enter{
  flex: 1;
  text-align: right;
  margin-right:40rpx;
  color: #FF004C;

}