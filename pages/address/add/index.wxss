}
.adress-add{
  font-size: 14px;
 
}
.adress-add-item{
  background: #fff;
  padding:0 20rpx;
  border-bottom: 1px solid #EAEAEA;
  height: 88rpx;
  line-height: 88rpx;
  position: relative;
  display: flex;
  margin-top: 20rpx;
  align-items: center;
}
.adress-add-item-sd{
    background: #fff;
  padding:0 20rpx;
  border-bottom: 1px solid #EAEAEA;
}
.adress-add-item input,.adress-add-item-sd input,.adress-add-item-sd label{
  font-size: 14px;
  height: 88rpx;
  line-height: 88rpx;
}
.adress-add-item input{
  padding-right: 40rpx;
}
