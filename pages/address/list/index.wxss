.item-s{
  padding: 20rpx;
  border-bottom: 1px solid #EAEAEA;
  background: white ;
  position: relative;
  
}
.item-s.unavaible{
  color: #999;
}
.address-list{
  margin-bottom: 88rpx;
}
.item-s .unImg{
  display: none;
  width: 128rpx;
  height: 128rpx;
  position: absolute;
  right: 130rpx;
  top:16rpx;
  z-index: 99;
}
.item-s.unavaible .unImg{
  display: inline-block;
}
.eidt-h{
  border-left: 1px solid #ddd;
  padding-left: 30rpx;
}
.addres-name{
  margin-top: 10rpx;
}
.phone{
  margin-left: 20rpx;
}
.address-add-save{
  height: 88rpx;
  align-items:center;
  left: 0;
  bottom: 0px;
  display: -webkit-flex;
  position:fixed;
  width: 100%;
  background: white;
  text-align: center;
  border-top: 1px solid  #FF5E5E;
}

.address-add-save-btn{
  background: #FF004C;
  width: 100%;
  color: white;
  border-radius: 0px;
} 
.address-add-save .ic-add{
  vertical-align: middle;
  margin-right: 10rpx;
  margin-top: -8rpx;
}
.empty-address{
text-align:center;
position:absolute;
width:100%;
top:10%;
}
.empty-address .empty-image{
width:400rpx;
height:400rpx;
display:block;
margin:0 auto;

}
.empty-address  .text{
color:#969696;
font-size:13px;

}
.empty-address .add-addres{
  margin-top: 40rpx;
  width: 60%;
  font-size: 28rpx;

}